{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "module": "commonjs",
    "target": "es2018",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
